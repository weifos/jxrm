/* pages/home/productDetail/productDetail.wxss */
@import "../../../modules/wxParse/wxParse.wxss";
swiper{
  height: 550rpx;
}
.product-info{
  padding: 20rpx;
}
.product-info__name{
  font-size: 33rpx;
  font-weight: bold;
  margin-top:10rpx;
}
.product-info__desc{
  font-size: 28rpx;
  color: #999;
  padding:10rpx 0 20rpx 0;
}
.tab-t{
  font-size: 0;
  margin-bottom: 20rpx;
}
.tab-t-item{
  width: 50%;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
  padding: 20rpx 0;
}

.tab-t-item.active{
  color: #4786e3;
  font-weight: bold;
  position: relative;
}

.tab-t-item.active:after{
  content:"";
  position: absolute;
  width: 40%;
  height: 4rpx;
  background-color: #4786e3;
  left: 50%;
  bottom: 0;
  transform:translateX(-50%);
}

.tab-c{
  font-size: 30rpx;
}
.tab-c-item{
  margin-bottom: 0rpx;
}
.i-collect{
  
  background: url(data:image/png;base64,) no-repeat 0 0 /100% auto;
  width: 36rpx;
  height: 32rpx;
  margin-right:10rpx;
}

.collected .i-collect{
  background-image: url(data:image/png;base64,);
}

.dib{
  display: inline-block;
}

.vam{
  vertical-align: middle;
}

.detail-tool-bar{
  position: fixed;
  bottom: -1rpx;
  font-size: 0;
  white-space: nowrap;
  width: 100%;
  text-align: center;
  border-top: 1px solid #069e40;
}
.detail-tool-bar .phone{
  width: 70%;
  font-size: 36rpx;
  background-color: #069e40;
  color: #fff;
  display: inline-block;
  height: 80rpx;
  line-height:80rpx;
  font-weight: bold;
}
.detail-tool-bar .product-info__collect{
  width: 30%;
  font-size: 24rpx;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  background:#ffffff;
}
.vat{
  vertical-align: top;
}